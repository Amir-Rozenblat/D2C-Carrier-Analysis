# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Override default models
OPENAI_GPT_MODEL=gpt-4-turbo-preview
OPENAI_EMBEDDING_MODEL=text-embedding-3-small

# Optional: File upload limits (in bytes)
NEXT_PUBLIC_MAX_FILE_SIZE=10485760  # 10MB
NEXT_PUBLIC_MAX_FILES=10

# Optional: Cost estimation (per 1M tokens)
OPENAI_GPT4_INPUT_COST=30.00
OPENAI_GPT4_OUTPUT_COST=60.00
OPENAI_EMBEDDING_COST=0.02

# Optional: Clustering configuration
DEFAULT_CLUSTER_K=5
MAX_CLUSTER_K=20
